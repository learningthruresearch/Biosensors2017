int val;
int brightness = 0;
int sensorpin=A0;

void setup()

{
pinMode(sensorpin, INPUT);
Serial.begin(9600); // sets the serial port to 9600

}

void loop()

{

val = analogRead(sensorpin); // read analog input pin 0
brightness = map(val, 0, 1023, 0, 255);
Serial.println(brightness);

delay(400);

}
